package collectionsdemo;

public class Product {
	private int code;
	private String name;
	private String description;
	private double price;
	private String manufacturer;
	
	public Product(int code,String name,String description,double price,String manufacturer)
	{
		this.code=code;
		this.name=name;
		this.description=description;
		this.price=price;
		this.manufacturer=manufacturer;
	
	}
	
	public String information() {
		return " code "+this.code+ " Name " +this.name+ " Desicription "+this.description+ " Price "+this.price+ " Manufacturer "+this.manufacturer;
	}
}
